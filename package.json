    "ytdl-core": "^4.11.4"
  },
  "devDependencies": {
    "@types/ffmpeg": "^1.0.4",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/node": "^20.3.1",
    "@types/npm": "^7.19.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.22",
