    "fluent-ffmpeg": "^2.1.2",
    "genius-lyrics": "^4.4.3",
    "mongodb": "^4.3.1",
    "node": "^20.2.0",
    "node-fetch": "^2.6.11",
    "npm": "^9.7.1",
    "prism-media": "^1.3.5",
